<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.SelfHomeApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@android:color/system_accent3_400</item>
        <item name="colorPrimaryVariant">@android:color/system_accent3_600</item>
        <item name="colorOnPrimary">@android:color/system_accent3_200</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@android:color/system_accent1_300</item>
        <item name="colorSecondaryVariant">@android:color/system_accent1_600</item>
        <item name="colorOnSecondary">@android:color/system_accent1_100</item>
        <item name="colorOnSurface">@android:color/system_accent1_800</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="appTitleColor">?attr/colorOnSurface</item>

        <item name="layout_backgroundColor">@android:color/system_accent3_50</item>

        <item name="importantColor">@color/my_red</item>

